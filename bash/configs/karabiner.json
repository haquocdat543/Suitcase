{
  "global": {
    "show_in_menu_bar": false
  },
  "profiles": [
    {
      "name": "Default",
      "complex_modifications": {
        "rules": [
          {
            "description": "Initialization",
            "manipulators": [
              {
                "description": "Open Slack with right command + S",
                "from": {
                  "key_code": "s",
                  "modifiers": {
                    "mandatory": ["right_command"]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Slack"
                  }
                ],
                "type": "basic"
              },
              {
                "description": "Open Telegram with right command + T",
                "from": {
                  "key_code": "t",
                  "modifiers": {
                    "mandatory": ["right_command"]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Telegram"
                  }
                ],
                "type": "basic"
              },
              {
                "description": "Open Zalo with right command + Z",
                "from": {
                  "key_code": "z",
                  "modifiers": {
                    "mandatory": ["right_command"]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Zalo"
                  }
                ],
                "type": "basic"
              },
              {
                "description": "Open Chrome with right command + C",
                "from": {
                  "key_code": "c",
                  "modifiers": {
                    "mandatory": ["right_command"]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a 'Google Chrome.app'"
                  }
                ],
                "type": "basic"
              }
            ]
          }
        ]
      }
    }
  ]
}
